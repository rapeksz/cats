{
    "name": "rszewc/thecats",
    "description": "A simple cats API library for Laravel",
    "type": "library",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "psr/log": "^1",
        "psr/http-message": "^1",
        "psr/http-client": "^1",
        "guzzlehttp/guzzle": "^7",
        "illuminate/support": "^7"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "src"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rszewc\\Thecats\\CatsServiceProvider"
            ]
        }
    }
}
